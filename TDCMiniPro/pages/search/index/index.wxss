page {
	width: 100%;
	height: 100%;
}
.searchBox {
	position: fixed;
	justify-content: center;
	width: 100%;
	height: 108rpx;
	padding: 0px 5%;
	top: 0px;
	left: 0px;
	background-color: #ffffff;
	z-index: 10;
}
.searchBox::after {
	content: "";
	position: absolute;
	width: 90%;
	bottom: 0px;
	left: 5%;
	height: 1px;
	background-color: #CCCCCC;
}
.searchIcon {
	width: 40rpx;
	height: 40rpx;
	margin-right: 10rpx;
}
.searchInput {
	width: calc(100% - 190rpx);
	height: 108rpx;
	margin-right: 10rpx;
}
.searchBtn {
	width: 130rpx !important;
	height: 60rpx;
	margin: 0px !important;
	font-size: 32rpx;
	font-weight: normal;
	line-height: 60rpx;
	background-color: #F5F5F5;
	color: #000000;
	z-index: 10;
}
.resultBox {
	box-sizing: border-box;
    width: 100%;
    height: 100%;
    padding-top: 150rpx;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
}
.resultWrap {
	display: none;
}
.resultWrap.show {
	display: block;
}
.marginBtom {
	margin-bottom: 30rpx;
}
.outsideBoxshadow {
	margin-bottom: 70rpx;
	box-shadow: 0px 15px 20px 0px rgba(18,51,118,0.15); 
}
.proType {
	margin-bottom: 24rpx;
	font-size: 28rpx;
}