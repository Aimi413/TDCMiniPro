.questionListBox {
	padding-bottom: 250rpx;
}
.programBox {
	padding: 50rpx 0px;
	background-color: var(--themeColor2);
}
.programName {
	margin-bottom: 36rpx;
	font-size: 32rpx;
	font-weight: bold;
	line-height: 52rpx;
	color: #ffffff;
}
.questionNum {
	font-size: 24rpx;
	color: #FFFFFF;
}
.questionList {
	padding-top: 36rpx;
	border-radius: 10px 10px 0px 0px;
	background-color: #ffffff;
	overflow: hidden;
}
.emptyAsk {
	display: block;
	width: 240rpx;
	height: 240rpx;
	margin: 200rpx auto 0px;
}
.emptyDesc {
	margin: 40rpx auto 0px;
	font-size: 28rpx;
	font-size: 28rpx;
    text-align: center;
    color: #999999;
}
.yellowBg {
	background-color: #FFF6E7;
}
.grayBg {
	background-color: #F8F8F8;
}
.openAskWin {
	position: fixed;
	width: 300rpx;
	height: 100rpx;
	bottom: 60rpx;
	left: 50%;
	transform: translateX(-50%);
	font-size: 36rpx;
	line-height: 100rpx;
	text-align: center;
	border-radius: 4px;
	background-color: var(--themeColor);
	color: #ffffff;
}
.askInput {
	box-sizing: border-box;
	width: 100%;
	height: 270rpx;
	padding: 20rpx;
	background-color: #F8F8F8;
}
.submitAsk {
	width: 300rpx !important;
	height: 100rpx;
	margin: 40rpx auto !important;
	font-size: 32rpx;
	font-weight: normal;
	line-height: 100rpx;
	border-radius: 4px;
	background-color: var(--themeColor);
	color: #ffffff;
}
.goDynamic {
	position: fixed;
	bottom: 200rpx;
	right: 20rpx;
	z-index: 10;
}
.goDynamicIcon {
	width: 138rpx;
	height: 138rpx;
}