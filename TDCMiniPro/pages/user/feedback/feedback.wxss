.feedbackTitle {
	margin: 60rpx auto;
	font-size: 32rpx;
	font-weight: bold;
	text-align: center;
}
.normalInput {
	width: 100%;
	height: 80rpx;
	margin-bottom: 20rpx;
	padding: 0px 20rpx;
	background-color: #f8f8f8;
}
.feedbackInput {
	box-sizing: border-box;
	width: 100%;
	height: 300rpx;
	margin: 20rpx auto 60rpx;
	padding: 20rpx;
	background-color: #f8f8f8;
}
.submitFeedback {
	width: 300rpx;
	height: 100rpx;
	margin: 40rpx auto 0px;
	font-size: 32rpx;
	font-weight: bold;
	line-height: 100rpx;
	text-align: center;
	border-radius: 4px;
	background-color: var(--themeColor);
	color: #ffffff;
}