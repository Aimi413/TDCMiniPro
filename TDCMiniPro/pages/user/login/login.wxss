page {
	width: 100%;
	height: 100%;
}
.login {
	width: 100%;
	height: 100%;
	position: relative;
	background-color: #FFFFFF;
}
.loginBox {
	position: absolute;
	width: 80%;
	margin: 0 auto;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	z-index: 15;
}
.loginLogo {
	text-align: center;
}
.loginLogoImg {
	width: 520rpx;
	height: 70rpx;
}
.login-wechatPhone {
	margin: 188rpx auto 80rpx;
}
.wechatPhone {
	width: 100%;
	height: 80rpx;
	font-size: 28rpx;
	line-height: 80rpx;
	border-radius: 0;
	background-color: var(--themeColor);
	color: #fff;
}
.noLogin {
	font-size: 32rpx;
	line-height: 48rpx;
	color: #666;
	text-align: center;
	display: block;
}
.wechatPhone image {
	width: 36rpx;
	height: 36rpx;
	vertical-align: middle;
}
.wechatPhone text {
	margin-left: 17rpx;
	vertical-align: middle;
}
.login-wechatPhoneTips {
	margin-top: 20rpx;
	text-align: center;
	font-size: 20rpx;
	line-height: 30rpx;
	color: #999;
}
.login-rests {
	height: 14rpx;
	border-bottom: 2rpx #f0f1f2 solid;
	font-size: 28rpx;
	line-height: 28rpx;
	color: #666;
	text-align: center;
	margin-bottom: 50rpx;
}
.login-rests text {
	display: inline-block;
	background: #fff;
	padding: 0 24rpx;
}
.login-otherItem {
	width: 46%;
	font-size: 28rpx;
	line-height: 88rpx;
	color: #666;
	border: 2rpx #f0f1f2 solid;
	text-align: center;
}
.login-otherItem image {
	width: 33rpx;
	height: 33rpx;
	vertical-align: middle;
	margin-right: 22rpx;
}
.copy {
	width: 100%;
	font-size: 22rpx;
	line-height: 28rpx;
	color: #999;
	text-align: center;
	position: absolute;
	bottom: 27rpx;
	left: 0;
	z-index: 14;
}
.copy text {
	display: block;
}
.loginWin {
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	padding-top: 40rpx;
	background-color: #ffffff;
	z-index: 30;
}
.loginWinBox {
	position: relative;
	width: 93%;
	margin: 0 auto;
	padding: 80rpx 60rpx 40rpx;
	background-color: #fff;
}
.closeLoginWin {
	position: absolute;
	top: 30rpx;
	right: 30rpx;
	z-index: 10;
}
.loginForm {
	display: block;
	margin-top: 105rpx;
}
.formGroup {
	height: 120rpx;
}
.formInput {
	box-sizing: border-box;
	width: 100%;
	height: 80rpx;
	padding: 0 20rpx;
	font-size: 28rpx;
	line-height: 38rpx;
	border: 1px #999 solid;
}
.formTip {
	flex-shrink: 0;
	font-size: 24rpx;
	line-height: 40rpx;
	color: red;
}
.valicodeInput {
	width: 52%;
}
.getValicodeBtn {
	width: 45% !important;
	height: 80rpx;
	/* padding: 0px; */
	background-color: var(--themeColor);
	font-size: 28rpx;
	font-weight: normal;
	line-height: 80rpx;
	color: #fff;
	border-radius: 0;
}
.loginBtn {
	display: block;
	width: 100% !important;
	height: 84rpx;
	padding: 0px;
	font-size: 28rpx;
	line-height: 84rpx;
	border-radius: 0px;
	background-color: var(--themeColor);
	color: #FFFFFF;
}
.cantLoginTip {
	font-size: 24rpx;
	line-height: 40rpx;
}
.phoneNum {
	color: blue;
}
/* .popup-noLoginBtn {
	display: inline-block;
	font-size: 24rpx;
	line-height: 28rpx;
	color: #999;
} */
.cantLoginTipBox {
	position: absolute;
	width: 600rpx;
	padding: 60rpx 0px 0px;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	border-radius: 12px;
	overflow: hidden;
	background-color: #ffffff;
}
.cantLoginTipCaption {
	margin-bottom: 60rpx;
	font-size: 32rpx;
	font-weight: bold;
	text-align: center;
}
.cantLoginTipDesc {
	padding: 0px 40rpx;
	font-size: 28rpx;
	line-height: 40rpx;
}
.cantLoginTipBtns {
	margin-top: 40rpx;
	border-top: 1px solid #e5e5e5;
}
.cantLoginTipBtn {
	width: 50%;
	height: 100rpx;
	font-size: 32rpx;
	line-height: 100rpx;
	text-align: center;
}
.goFeedBackBtn {
	border-left: 1px solid #e5e5e5;
}