.profile {
	margin-bottom: 100rpx;
	padding: 50rpx 0px 0px;
	background: url("https://img.traveldaily.cn/events/app/2020/user/profileTopBg.png") top center/100% auto no-repeat;
	overflow: hidden;
}
.profileCard {
	box-sizing: border-box;
	position: relative;
	padding: 30rpx;
	border-radius: 4px;
	background: url("https://img.traveldaily.cn/events/app/2020/user/profileCardBg.png") center center no-repeat;
	background-size: cover;
	overflow: hidden;
}
.profileInfos {
	margin-bottom: 24rpx;
	font-size: 0px;
}
.profileInfo, .profileAvatar {
	display: inline-block;
	vertical-align: middle;
}
.profileAvatar {
	width: 156rpx;
	height: 156rpx;
	margin-left: 70rpx;
	border-radius: 100%;
}
.profileInfo {
	width: calc(100% - 226rpx);
}
.profileName {
	margin-bottom: 10rpx;
	font-size: 44rpx;
	font-weight: bold;
	color: var(--themeColor2);
}
.profileCompany {
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
	font-size: 24rpx;
	line-height: 48rpx;
	text-overflow: ellipsis;
	color: #666666;
}
.profileContact {
	margin-bottom: 20rpx;
	font-size: 28rpx;
	text-align: left;
	color: #666666;
}
.profileContact:last-child {
	margin-bottom: 0px;
}
.contactIcon {
	width: 28rpx;
	height: 28rpx;
	margin-right: 10rpx;
}
.goEditPage {
	position: absolute;
	width: 120rpx;
	height: 40rpx;
	right: 30rpx;
	bottom: 30rpx;
	font-size: 24rpx;
	line-height: 40rpx;
	text-align: center;
	border-radius: 20px;
	background-color: #000000;
	color: #FFFFFF;
}
.goEditIcon {
	display: inline-block;
	vertical-align: middle;
	width: 28rpx;
	height: 28rpx;
	margin-top: -4rpx;
}
.profileIntro {
	margin: 36rpx 0px 30rpx;
	padding-bottom: 36rpx;
	font-size: 28rpx;
	border-bottom: 1px solid #E5E5E5;
}
.profileMenu {
	box-sizing: border-box;
	height: 120rpx;
	padding: 40rpx 0px;
}
.menuIcon {
	width: 40rpx;
	height: 40rpx;
	margin-right: 30rpx;
}
.menuText {
	font-size: 32rpx;
	line-height: 40rpx;
	color: #000000;
}
.goMenuIcon {
	width: 24rpx;
	height: 24rpx;
	margin-top: 8rpx;
}
.menuMsg {
	margin-right: 20rpx;
	padding: 0px 20rpx;
	font-size: 24rpx;
	line-height: 40rpx;
	border-radius: 10px;
	background-color: var(--themeColor);
	color: #FFFFFF;
}
.menuDivide {
	height: 1px;
	margin: 28rpx 0px;
	background-color: #E5E5E5;
}

.dataTip {
	display: block;
	margin: 50rpx auto;
}
.closeDataTip {
	width: 300rpx;
	height: 108rpx;
	margin: 400rpx auto 0px;
	font-size: 36rpx;
	line-height: 108rpx;
	text-align: center;
	border-radius: 6px;
	background-color: #FFFFFF;
	color: #000000;
}
.servinceBox {
	position: absolute;
	width: 570rpx;
	padding: 60rpx 50rpx;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	border-radius: 6px;
	background-color: #FFFFFF;
}
.servinceTitle {
	margin-bottom: 60rpx;
	font-size: 38rpx;
	font-weight: bold;
	text-align: center;
}
.servinceDesc {
	margin-bottom: 70rpx;
	font-size: 32rpx;
	line-height: 50rpx;
	color: #666666;
}
.phoneNum {
	color: blue;
}
.servinceDesc:last-child {
	margin-bottom: 0px;
}